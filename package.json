  "scripts": {
    "clean": "rm -rf ./lib ./esm",
    "build": "npm run clean && npm run build:commonjs && npm run build:esm",
    "build:esm": "BABEL_ENV=esm babel src -d esm --extensions \".js,.jsx\" --copy-files",
    "build:commonjs": "BABEL_ENV=cjs babel ./src --extensions \".js,.jsx\" -d lib --copy-files",
    "prebuild": "npm run test",
    "prepare": "npm run build",
    "start:webdemo": "cd webDemo && npm i && npm run start",
    "start:watch": "nodemon -e js --watch src --exec 'npm run build'",
    "start": "concurrently \"npm run start:watch\" \"npm run start:webdemo\"",
    "test": ""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
